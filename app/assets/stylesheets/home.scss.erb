.card {
  border: 0;
  margin-bottom: 20px;
  margin-top: 20px;
  border-radius: 6px;
  color: #333;
  background: #fff;
  width: 100%;
  box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12);

  .card-category {
    &:not([class*=text-]) {
      color: #999;
