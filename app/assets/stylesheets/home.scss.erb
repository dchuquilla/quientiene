      background: linear-gradient(60deg, #26c6da, #00acc1);
    }
  }

  &.bg-success {
    background: linear-gradient(60deg, #66bb6a, #43a047);
  }

  .card-header-success {
    .card-icon, .card-text, &:not(.card-header-icon):not(.card-header-text) {
      background: linear-gradient(60deg, #66bb6a, #43a047);
    }
  }

  &.card-rotate.bg-success {
