<!-- ****************************** Contact Section ************************** -->
<section id="contact">
  <section class="container contact-wrap">
    <section class="row">
      <div class="title-box">
        <h1 class="block-title wow animated rollIn">
          <span class="bb-top-left"></span>
          <span class="bb-bottom-left"></span>
          Contáctenos
          <span class="bb-top-right"></span>
          <span class="bb-bottom-right"></span>
        </h1>
      </div>
    </section>
  </section>
  <section class="address">
    <div class="container">
      <div class="col-sm-12">
        <ul class="address-list">
          <% if false %>
            <li><i class="ion-ios-location" style="background-color: rgb(255, 102, 0);"></i> <span>1537 Flint Street <br>
                Tumon, MP 96911</span></li>
            <li><i class="ion-earth" style="background-color: #ff6969;"></i> <span>www.themewagon.com</span></li>
          <% end%>
          <li><i class="ion-ios-telephone" style="background-color: #63cfea;"></i> <span>+5939 6167 4787 </span></li>
          <li><a href="https://www.tiktok.com/@quientiene.com"><i class="ion-email" style="background-color: #6ecba9;"></i> <span>info@quientiene.com</span></a></li>
          <li><a href="https://www.facebook.com/people/Quien-Tiene/100073178387465/"><i class="ion-social-facebook" style="background-color: #4267B2;"></i> <span>Quien Tiene</span></a></li>
          <li><a href="https://twitter.com/QuienTiene_com"><i class="ion-social-twitter" style="background-color: #1DA1F2;"></i> <span>QuienTiene.com</span></a></li>
          <li><a href="https://www.tiktok.com/@quientiene.com"><i style="background-color: #000000;"><img src="data:image/svg+xml;base64," width="32" height="32" style="margin-top: -10px;" alt="Encuéntrenos en TikTok"></i> <span>QuienTiene.com</span></a></li>
        </ul>
      </div>
    </div>
  </section>
  <section class="mailbox">
    <div class="container">
      <div class="col-sm-12">
        <form name="sentMessage" id="contactForm" novalidate>
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Nombre *" id="name" required data-validation-required-message="Please enter your name.">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input type="email" class="form-control" placeholder="Correo Electrónico *" id="email" required data-validation-required-message="Please enter your email address.">
                <p class="help-block text-danger"></p>
              </div>
              <div class="form-group">
                <input type="tel" class="form-control" placeholder="Teléfono *" id="phone" required data-validation-required-message="Please enter your phone number.">
                <p class="help-block text-danger"></p>
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <textarea class="form-control" placeholder="Mensaje *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                <p class="help-block text-danger"></p>
                <div id="success"></div>
                <button type="submit" class="polo-btn contact-submit"><i class="ion-paper-airplane"></i></button>
              </div>
            </div>
          </div>
        </form>
      </div>
    </div>
  </section>
  <div class="clearfix"></div>
</section>
